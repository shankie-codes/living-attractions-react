{
  "name": "garden-living-attractions-react",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "version": "0.1.1",
  "scripts": {
    "start": "npm run watch",
    "watch": "watchify index.js -o ../livingattractions.js -dv --poll",
    "build": "NODE_ENV=production browserify index.js | uglifyjs -cm > ../livingattractions.min.js"
  },
  "dependencies": {
    "babelify": "^6.4.0",
    "browserify": "^10.2.6",
    "cookie-parser": "^1.4.0",
    "ecstatic": "~0.8.0",
    "errorhandler": "^1.4.2",
    "react": "~0.13.3",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  }
}
